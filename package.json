{
  "name": "electron-demo",
  "version": "1.3.20",
  "description": "electron demo",
  "main": "dist/main.js",
  "scripts": {
    "dev:render": "rsbuild dev",
    "dev:main": "npm run build:main && cross-env NODE_ENV=development electron ./dist/main.js",
    "build:render": "rsbuild build",
    "build:main": "tsc --build ./src/main/tsconfig.json",
    "build:publish": "electron-builder build --x64 && npm run upload:oss",
    "upload:oss": "ts-node ./scripts/upload.ts",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "precompiler": "cross-env NODE_ENV=production electron-builder build --x64",
    "watch:main": "tsc --build ./src/main/tsconfig.json -w"
  },
  "dependencies": {
    "@deno/kv": "^0.10.0",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.0.0",
    "@excalidraw/excalidraw": "^0.18.0",
    "@icon-park/react": "^1.4.2",
    "@reduxjs/toolkit": "^2.6.1",
    "@rsbuild/core": "^1.3.16",
    "@rsbuild/plugin-less": "^1.2.3",
    "@rsbuild/plugin-node-polyfill": "^1.3.0",
    "@rsbuild/plugin-react": "^1.3.0",
    "@rsbuild/plugin-sass": "^1.3.1",
    "@types/ali-oss": "^6.16.11",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.2",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "ali-oss": "^6.20.0",
    "antd": "^5.24.9",
    "axios": "^1.8.4",
    "codemirror": "^6.0.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "devtron": "^1.4.0",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "events": "^3.3.0",
    "qs": "^6.12.1",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xl07097/electron-demo.git"
  },
  "author": "xueliang",
  "license": "ISC",
  "build": {
    "productName": "创客",
    "appId": "com.xueliang.xiangshuye",
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://xiangshuye.oss-cn-shanghai.aliyuncs.com/software/install/"
      }
    ],
    "asarUnpack": [
      "exec",
      "node_modules/@deno"
    ],
    "asar": {
      "smartUnpack": true
    },
    "files": [
      "assets",
      "dist",
      "src/preload",
      "build",
      "exec"
    ],
    "win": {
      "icon": "assets/icons/256x256.ico",
      "artifactName": "${productName}-v${version}.${ext}",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "assets/icons/256x256.ico",
      "createDesktopShortcut": true
    }
  }
}
