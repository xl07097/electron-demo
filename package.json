{
  "name": "electron-demo",
  "version": "1.3.23",
  "description": "electron demo",
  "main": "dist/main/main.js",
  "scripts": {
    "dev:render": "rsbuild dev",
    "dev:main": "electron ./dist/main/main.js",
    "build:render": "rsbuild build",
    "build:main": "rspack build --mode production",
    "build:main:dev": "rspack build --mode development",
    "build:publish": "electron-builder build --x64 && npm run upload:oss",
    "upload:oss": "ts-node ./scripts/upload.ts",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "precompiler": "electron-builder build --x64",
    "watch:main": "tsc --build ./src/main/tsconfig.json -w"
  },
  "dependencies": {
    "@deno/kv": "^0.12.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.0.2",
    "@excalidraw/excalidraw": "^0.18.0",
    "@icon-park/react": "^1.4.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@rsbuild/core": "^1.5.13",
    "@rsbuild/plugin-node-polyfill": "^1.4.2",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@types/ali-oss": "^6.16.11",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.2",
    "@types/qs": "^6.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "ali-oss": "^6.20.0",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "codemirror": "^6.0.2",
    "cross-env": "^10.1.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "electron": "^38.2.0",
    "electron-builder": "^26.0.12",
    "events": "^3.3.0",
    "qs": "^6.14.0",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xl07097/electron-demo.git"
  },
  "author": "xueliang",
  "license": "ISC",
  "build": {
    "productName": "创客",
    "appId": "com.xueliang.electron-demo",
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://xiangshuye.oss-cn-shanghai.aliyuncs.com/software/install/"
      }
    ],
    "asarUnpack": [
      "assets/exec",
      "node_modules/@deno"
    ],
    "asar": {
      "smartUnpack": true
    },
    "files": [
      "dist/main",
      "dist/render",
      "dist/preload",
      "assets/icons",
      "assets/exec"
    ],
    "win": {
      "icon": "assets/icons/256x256.ico",
      "artifactName": "${productName}-v${version}.${ext}",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "assets/icons/256x256.ico",
      "createDesktopShortcut": true
    }
  }
}
