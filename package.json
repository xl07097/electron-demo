{
	"name": "electron-demo",
	"version": "1.3.7",
	"description": "electron demo",
	"main": "dist/main.js",
	"scripts": {
		"dev:render": "cd ./src/render && vite",
		"dev:main": "npm run build:main && cross-env NODE_ENV=development electron ./dist/main.js",
		"build:render": "cd ./src/render && tsc && vite build",
		"build:main": "tsc --build ./src/main/tsconfig.json",
		"build:publish": "electron-builder build --x64 && npm run upload:oss",
		"upload:oss": "ts-node ./scripts/upload.ts",
		"major": "npm version major",
		"minor": "npm version minor",
		"patch": "npm version patch",
		"precompiler": "cross-env NODE_ENV=production electron-builder build --x64",
		"watch:main": "tsc --build ./src/main/tsconfig.json -w"
	},
	"dependencies": {
		"@deno/kv": "^0.7.0",
		"electron-log": "^5.1.4",
		"electron-updater": "^6.1.8",
		"tslib": "^2.6.2"
	},
	"devDependencies": {
		"@ant-design/icons": "^5.3.7",
		"@icon-park/react": "^1.4.2",
		"@reduxjs/toolkit": "^2.2.4",
		"@types/ali-oss": "^6.16.11",
		"@types/crypto-js": "^4.2.2",
		"@types/node": "^18.19.2",
		"@types/qs": "^6.9.15",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.2.1",
		"ali-oss": "^6.20.0",
		"antd": "^5.17.2",
		"axios": "^1.6.8",
		"cross-env": "^7.0.3",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.11",
		"devtron": "^1.4.0",
		"electron": "^30.0.4",
		"electron-builder": "^24.13.3",
		"events": "^3.3.0",
		"less": "^4.2.0",
		"qs": "^6.12.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-infinite-scroll-component": "^6.1.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.23.1",
		"redux": "^5.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5",
		"unplugin-vue-components": "^0.27.0",
		"vite": "^4.5.0"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:xl07097/electron-demo.git"
	},
	"author": "xueliang",
	"license": "ISC",
	"build": {
		"productName": "创客",
		"appId": "com.xueliang.xiangshuye",
		"directories": {
			"output": "release"
		},
		"publish": [
			{
				"provider": "generic",
				"url": "https://xiangshuye.oss-cn-shanghai.aliyuncs.com/software/install/"
			}
		],
		"asarUnpack": [
			"exec",
			"node_modules/@deno"
		],
		"asar": {
			"smartUnpack": true
		},
		"files": [
			"assets",
			"dist",
			"src/preload",
			"build",
			"exec"
		],
		"win": {
			"icon": "assets/icons/256x256.ico",
			"artifactName": "${productName}-${version}.${ext}",
			"target": "nsis"
		},
		"nsis": {
			"oneClick": false,
			"perMachine": false,
			"allowToChangeInstallationDirectory": true,
			"allowElevation": true,
			"deleteAppDataOnUninstall": true,
			"installerIcon": "assets/icons/256x256.ico",
			"createDesktopShortcut": true
		}
	}
}
